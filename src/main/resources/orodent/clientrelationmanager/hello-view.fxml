<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="orodent.clientrelationmanager.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <TextField fx:id="searchField" />
   <Label fx:id="status" />

    <Label fx:id="resultField" />
   <Button fx:id="pingButton" mnemonicParsing="false" onAction="#onPingButtonClick" text="Ping" />
   <HBox spacing="20.0">
      <children>
          <Button onAction="#onHelloButtonClick" text="Start Server" />
         <Button mnemonicParsing="false" onAction="#onStopServer" text="Stop Server" />
      </children>
   </HBox>
   <Button mnemonicParsing="false" onAction="#onEmbeddedTest" text="embedded test" />
</VBox>
